<UserControl x:Class="PokemonScriptStudio.VisualRom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid MouseLeftButtonUp="Grid_MouseLeftButtonUp">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="20"/>
		</Grid.RowDefinitions>
		<Image x:Name="imgIcoRom"/>
		<Viewbox  Grid.Row="1">
			<TextBlock x:Name="txtNombreRom"/>
		</Viewbox>
	</Grid>
</UserControl>